<Window x:Class="salon.Admin.AdminControl.AddEmployerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:salon.Admin.AdminControl"
        mc:Ignorable="d"
        Title="AddEmployerWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <TextBox Name="name" ></TextBox>
        <TextBox Grid.Row="1" Name="age"></TextBox>
        <TextBox Grid.Row="2" Name="possition"></TextBox>
        <Button Click="AddImg_OnClick" Grid.Row="3">Добавить Иконку</Button>
        <Button Click="Add_OnClick" Grid.Row="4">Добавить</Button>
    </Grid>
</Window>
